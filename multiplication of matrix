#include<stdio.h>
int main()
{
	int a[2][2]={{1,2},{2,3}};
	int b[2][2]={{5,6},{8,9}};
	int c[2][2];
	int i,j,k;
	for(i=0;i<2;i++)
	{
		for(j=0;j<2;j++)
		{
			c[i][j]=0;
			for(k=0;k<2;k++)
			{
				c[i][j]+=a[i][j]*b[i][j];
			}
		}
	}
	printf("matrix obtained is \n");
	for(i=0;i<2;i++)
	{
		for(j=0;j<2;j++)
		{
			printf("%d\t",c[i][j]);
		}
		printf("\n");
	}
	return 0;
}
