#include<stdio.h>
int main()
{
	int i,p,n;
	int a[20];
	printf("enter the elements int array");
	for(i=0;i<9;i++)
	{
		scanf("%d",&a[i]);
	}
	printf("enter the number to insert");
	scanf("%d",&n);
	printf("enter the position to enter");
	scanf("%d",&p);
	for(i=9;i<=p;i--)
	{
		a[i+1]=a[i];
	}
	  a[p]=n;

	for(i=0;i<9;i++)
	{
		printf("%d ",a[i]);
	}
	return 0;
}
