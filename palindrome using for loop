#include <stdio.h>

int main()
{
    int n, temp, rev = 0, digit;

    printf("Enter an integer: ");
    scanf("%d", &n);

    temp = n;
    for (; n != 0; n = n / 10)
    {
        digit = n % 10;
        rev = rev * 10 + digit;
    }

    if (temp == rev){
        printf("The given number is a Palindrome.");}
       
        
    else{
        printf("The given number is not a Palindrome.");
}
    return 0;
}
